                      // 18\nMeteor.startup(Template.body.renderToDocument);                                                     // 19\n                                                                                                    // 20\nTemplate.__checkName(\"pelicula\");                                                                   // 21\nTemplate[\"pelicula\"] = new Template(\"Template.pelicula\", (function() {                              // 22\n  var view = this;                                                                                  // 23\n  return HTML.LI({                                                                                  // 24\n    class: function() {                                                                             // 25\n      return Blaze.If(function() {                                                                  // 26\n        return Spacebars.call(view.lookup(\"checked\"));                                              // 27\n      }, function() {                                                                               // 28\n        return \"checked\";                                                                           // 29\n      });                                                                                           // 30\n    }                                                                                               // 31\n  }, \"\\n    \", HTML.INPUT({                                                                         // 32\n    type: \"checkbox\",                                                                               // 33\n    checked: function() {                                                                           // 34\n      return Spacebars.mustache(view.lookup(\"checked\"));                                            // 35\n    },                                                                                              // 36\n    class: \"toggle-checked\